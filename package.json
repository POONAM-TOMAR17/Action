{
  "name": "actonate-website",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "build": "gatsby build",
    "build-analyze": "ANALYZE=true gatsby build",
    "build-tailwind": "npx tailwindcss build -i ./styles/tailwind.css -o ./styles/main.css",
    "start": "npm run build-tailwind && ANALYZE=true npm run build --max-old-space-size=2048",
    "ts-lint": "tslint -c tslint.json --project tsconfig.json \"src/**/*.ts?(x)\"",
    "lint-fix": "tslint --fix --project tsconfig.json --format stylish",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.3.3",
    "autoprefixer": "10.3.4",
    "babel-eslint": "10.1.0",
    "cssnano": "5.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-react": "7.25.1",
    "gatsby-plugin-eslint": "2.0.8",
    "gatsby-plugin-tsconfig-paths": "^1.0.3",
    "postcss": "8.3.4",
    "prettier": "2.3.2",
    "tailwindcss": "2.2.4"
  },
  "dependencies": {
    "@egjs/flicking-plugins": "^4.2.1",
    "@egjs/react-flicking": "^4.3.0",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.5",
    "@hookstate/core": "^3.0.11",
    "@tailwindcss/ui": "^0.7.2",
    "@types/eslint": "^7.28.1",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/react-collapse": "^5.0.1",
    "@types/react-helmet": "^6.1.2",
    "@types/react-modal": "^3.12.1",
    "@types/react-slick": "^0.23.5",
    "@types/styled-components": "^5.1.14",
    "gatsby": "2.32.12",
    "gatsby-plugin-manifest": "2.12.1",
    "gatsby-plugin-offline": "3.10.2",
    "gatsby-plugin-postcss": "3.7.0",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-sharp": "2.14.3",
    "gatsby-plugin-styled-components": "^4.13.0",
    "prop-types": "15.7.2",
    "query-string": "^7.0.1",
    "react": "17.0.2",
    "react-collapse": "^5.1.0",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.4.2",
    "react-helmet": "^6.1.0",
    "react-modal": "^3.14.3",
    "react-select": "^5.1.0",
    "react-slick": "^0.28.1",
    "styled-components": "^5.3.1",
    "tailwindcss-pseudo-elements": "^2.0.0",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^4.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "prettier --write"
  }
}
